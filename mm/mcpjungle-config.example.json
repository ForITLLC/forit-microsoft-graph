{
  "name": "mm",
  "transport": "stdio",
  "description": "Microsoft PowerShell. Omit all params to list connections. Provide connection+module+command to execute.",
  "command": "/path/to/your/mm/.venv/bin/python",
  "args": [
    "/path/to/your/mm/server.py"
  ],
  "env": {
    "MM_SESSION_POOL_URL": "http://localhost:5200",
    "PATH": "/opt/homebrew/bin:/opt/homebrew/sbin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
  },
  "session_mode": "stateless"
}
